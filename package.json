{
  "name": "url-collector",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start:dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start:prod": "node lib/index.js",
    "build:clean": "rimraf lib/",
    "build:compile": "tsc --project ./tsconfig.build.json",
    "build": "npm run build:clean && npm run build:compile",
    "lint": "eslint ./src --ext .js,.ts --fix",
    "test": "npm run test:unit",
    "test:unit": "jest --verbose --config=unit.jest.config.ts",
    "test:unit:watch": "jest --verbose --watchAll --config=unit.jest.config.ts"
  },
  "dependencies": {
    "@tshio/command-bus": "^1.0.4",
    "@tshio/query-bus": "^1.0.4",
    "async-await-queue": "^1.2.1",
    "awilix": "^7.0.3",
    "celebrate": "^15.0.1",
    "cors": "^2.8.5",
    "cross-fetch": "^3.1.5",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "helmet": "^6.0.1",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@leocode/eslint-config": "^2.0.0",
    "@types/config": "^0.0.41",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.1.2",
    "@types/lodash": "^4.14.191",
    "@types/node": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.2.0",
    "nock": "^13.3.0",
    "prettier": "2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  }
}
